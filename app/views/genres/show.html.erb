
<h1><%= @genre.name %>の投稿一覧</h1>


<% @histories.each do |history| %>

 <p>内容</p>
 <span><%= history.body %></span>

 <p>投稿写真</p>
  <% if @history.histories.present? %>
  <% @history.histories.each do |image| %>
    <%= attachment_image_tag image, :image, :fill, 200, 200 %>
  <% end %>
  <% else %>
    <%= image_tag 'no_image.jpg', size: '200x200' %>
<% end %>

<% end %>
